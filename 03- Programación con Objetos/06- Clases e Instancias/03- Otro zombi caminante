module  Bouba
  @salud=100
  def self.gritar
    "¡agrrrg!"
    end
  def self.salud
    @salud
    end
  def self.sabe_correr?
    FALSE
    end
  def self.recibir_danio!(
    puntos)
    @salud=[(@salud-=(2*puntos)),0].max
    end
  end 
module Kiki
