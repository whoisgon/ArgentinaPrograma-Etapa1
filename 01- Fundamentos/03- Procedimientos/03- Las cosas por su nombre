procedure DibujarCuadradoNegroDeLado3() {
  Poner(Negro)
  Mover(Este)
  Poner(Negro)
  Mover(Este)
  Poner(Negro)
  Mover(Norte)
  Poner(Negro)
  Mover(Oeste)
  Poner(Negro)
  Mover(Oeste)
  Poner(Negro)
  Mover(Norte)
  Poner(Negro)
  Mover(Este)
  Poner(Negro)
  Mover(Este)
  Poner(Negro)    
}

program {
  DibujarCuadradoNegroDeLado3()   
}
